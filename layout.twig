				<a class="db dtc-l v-mid mid-gray link underline-hover w-100 h3 w-25-l tc tl-l mb2 mb0-l" href="{{ base_url }}" title="Home">
					{% if logo %}
						<img src="{{ logo }}" class="dib h-100"/>
					{% else %}
						<p class="dib f4 fw9">{{ settings.title }}</p>
					{% endif %}
				</a>
				<div class="db dtc-l v-mid w-100 w-75-l tc tr-l">

				  	{% for key,value in settings.themes.knowledge.navilinks %}
				  		{% set label = "navilabel" ~ loop.index %}
				  		{% set link = "navilink" ~ loop.index %}

				  		{% if loop.last %}
						    <a class="link underline-hover dark-gray f6 f5-l dib" href="{% if settings.themes.knowledge[link]|first == '/' %}{{base_url}}{% endif %}{{ settings.themes.knowledge[link] }}">{{ settings.themes.knowledge[label] }}</a>
				  		{% else %}
				    		<a class="link underline-hover dark-gray f6 f5-l dib mr3 mr4-l" href="{% if settings.themes.knowledge[link]|first == '/' %}{{base_url}}{% endif %}{{ settings.themes.knowledge[link] }}">{{ settings.themes.knowledge[label] }}</a>
				  		{% endif %}	

				  	{% endfor %}

				</div>
			</nav>
			
